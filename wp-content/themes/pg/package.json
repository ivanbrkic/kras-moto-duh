{
  "name": "playground",
  "version": "2.2.0",
  "description": "WordPress starter theme",
  "private": true,
  "scripts": {
    "mix-domain": "echo {\"url\": \"https://example.test\"} > mix-domain.json",
    "lint": "npm run lint-php && npm run lint-scss && npm run lint-js",
    "lint-php": "phpcs . --report=source",
    "lint-scss": "npx stylelint assets/src/**/*.scss",
    "lint-js": "npx eslint --ext .js assets/src/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Neuralab/Playground.git"
  },
  "keywords": [
    "WordPress",
    "starter",
    "theme"
  ],
  "author": "Development @ Neuralab",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Neuralab/Playground/issues"
  },
  "homepage": "https://neuralab.net",
  "devDependencies": {
    "browser-sync": "^2.27.11",
    "browser-sync-webpack-plugin": "^2.3.0",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "eslint": "^8.32.0",
    "eslint-config-xo": "^0.43.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-unicorn": "^45.0.2",
    "image-minimizer-webpack-plugin": "^3.8.1",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-mozjpeg": "^10.0.0",
    "imagemin-optipng": "^8.0.0",
    "imagemin-svgo": "^10.0.1",
    "laravel-mix": "^6.0.49",
    "laravel-mix-clean": "^0.1.0",
    "laravel-mix-svg-sprite": "^2.0.0",
    "postcss-discard-comments": "^5.1.2",
    "postcss-preset-env": "^7.8.3",
    "sass": "^1.57.1",
    "sass-loader": "^13.2.0",
    "stylelint": "^14.16.1",
    "stylelint-config-standard-scss": "^6.1.0"
  },
  "dependencies": {
    "bootstrap": "^4.6.2",
    "normalize.css": "^8.0.1",
    "sass-to-js": "^2.0.0",
    "swiper": "^9.4.1",
    "vanilla-cookieconsent": "^3.1.0"
  },
  "engines": {
    "node": ">=12.14.0"
  }
}
